{
    "stylePrefix": "/styles/",
    "scriptPrefix": "/scripts/",
    "uploadSizeLimit": 20971520,
    "analyticsKey": "xxxxxxxxx",
    "enableAnalyticsOnProof": false,
    "hitPageProofCount" : false,
    "idleSessionExpirationTime": 180000,
    "supportedExtension" : ["odt","ods", "odp", "odg", "odc", "odf", "odi", "odm", "doc",
        "dot", "docx", "dotx", "docm", "dotm", "xls", "xlt", "xlsx", "xla",
        "ppt", "pot", "pps", "ppa", "pptx", "potx", "ppsx" , "txt", "gif", "jpg",
        "png", "jpeg", "pjpeg", "avi", "mpeg", "ogg", "mp4", "mov", "webm",
        "flv", "mkv", "mk3d", "mka", "mks", "wma", "wmv", "asf", "wax", "wvx", "asx",
        "avi", "bz2", "bz", "mp3", "mpp", "pov", "ppm", "zip", "jsp", "pdf", "gzip", "html", "xml", "7z", "tif", "tiff"],
    "browserCompatible": {
        "Chrome": "35.0",
        "Firefox": "29.0",
        "InternetExplorer" : "11.0"
     },
    "locale": false,
    "localeEnable": ["AU", "MC", "JM", "ED"],
    "localeSetting": [
        {
            "name": "en-US",
            "title": "English",
            "imageUrl": "/images/en.png",
            "enable": true
         },
        {
            "name": "es-ES",
            "title": "Español",
            "imageUrl": "/images/es.png",
            "enable": true
        },
        {
            "name": "fr-FR",
            "title": "Français",
            "imageUrl": "/images/fr.png",
            "enable": true
        }
    ],
    "imageAnnotateBoxSize": {"height": 50, "width" : 50},
    "inlineImageAnnotateBoxSize": {"height": 20, "width" : 20},
    "mathAnnotateBoxSize": {"height": 18, "width" : 18},
    "highResImageAnnotateBoxSize": {"height": 30, "width" : 30},
    "disableXmlEditorProofView": true,
    "pingInterval": 10000,
    "pingTimeout": 5000
}
